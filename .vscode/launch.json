{
  "configurations": [
      {
          "type": "dezog",
          "request": "launch",
          "name": "DeZog",
          "remoteType": "cspect",
          "sjasmplus": [
            {
                "path": "${workspaceRoot}/project/src/myapp.sld.txt",
            }
          ],
          "startAutomatically": false,
          "history": {
              "reverseDebugInstructionCount": 10000,
              "codeCoverageEnabled": true
          },
          "commandsAfterLaunch": [
              //"-sprites",
              //"-patterns"
